{
  "name": "electron-vite-project",
  "version": "2.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite --force --host 0.0.0.0",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "rebuild": "npm rebuild --runtime=electron --target=21.1.1 --disturl=https://atom.io/download/atom-shell --abi=48"
  },
  "dependencies": {
    "electron": "^21.1.1",
    "electron-builder": "^21.1.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0",
    "vue-router": "^4.1.6",
    "element-plus": "^2.3.3"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^3.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.14",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "sass": "^1.49.9"
  }
}
